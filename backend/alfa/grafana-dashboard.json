{
  "dashboard": {
    "id": null,
    "title": "Warhammer Application Metrics",
    "tags": ["warhammer", "spring-boot"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Emails Sent",
        "type": "stat",
        "targets": [
          {
            "expr": "emails_sent_total",
            "legendFormat": "Emails Sent"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Emails Failed",
        "type": "stat",
        "targets": [
          {
            "expr": "emails_failed_total",
            "legendFormat": "Emails Failed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Email Sending Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(emails_sending_duration_seconds_sum[5m]) / rate(emails_sending_duration_seconds_count[5m])",
            "legendFormat": "Avg Duration (s)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "User Registrations",
        "type": "stat",
        "targets": [
          {
            "expr": "users_registrations_total",
            "legendFormat": "Registrations"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "User Logins",
        "type": "stat",
        "targets": [
          {
            "expr": "users_logins_total",
            "legendFormat": "Logins"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
} 